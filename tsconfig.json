{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "rootDir": "./src",
    "allowJs": true,
    "outDir": "./build",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true, 
    "skipLibCheck": true   
  },
  "include": ["./src/**/*", "src/utils/prueba.ts"],
  "exclude": ["./public", "./cypress", "cypress.config.ts"],

}
